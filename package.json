{
  "name": "breakfast",
  "version": "0.0.1",
  "description": "Quick text-based images",
  "main": "src/app.js",
  "scripts": {
    "test": "mocha",
    "build:sports": "babel-node --presets es2015 ./data/getSportsData",
    "build:logos": "node ./generateLogoJson",
    "build:dev": "npm run build:logos; npm run build:server; npm run build:js; npm run build:css",
    "build:js:min": "NODE_ENV=production globify -g uglifyify \"src/client/*.js\" --outfile=public/js",
    "build:css:min": "echo \"console.log('CSS DOES NOT MINIFY YET (figure me out)!')\" | node & npm run build:css",
    "build:min": "npm run build:logos; npm run build:server; npm run build:js:min; npm run build:css:min",
    "build:js": "globify -d \"src/client/*.js\" --outfile=public/js",
    "build:css": "postcss --map -c postcss.json src/css/*.css",
    "build:server": "babel src/server/  --out-dir dist",
    "watch:js": "globify -d \"src/client/*.js\" --outfile=public/js --watch",
    "watch:css": "postcss -mwc postcss.json src/css/*.css",
    "watch:server": "echo \"console.log('node-dev options need browserifyflag (figure me out)!')\" | node & npm run server",
    "watch": "npm run watch:js & npm run watch:css",
    "server": "npm run build:server; node dist/server.js",
    "dev": "npm run watch & npm run watch:server",
    "start": "npm build:min & DEBUG=breakfast:* npm run server"
  },
  "browserify": {
    "extensions": [
      ".js"
    ],
    "transform": [
      "babelify"
    ],
    "resolve": {}
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.1.2",
    "aws-sdk": "^2.2.25",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.13.3",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.10",
    "chance": "^0.7.6",
    "cheerio": "^0.19.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.3.5",
    "csurf": "^1.8.3",
    "d3": "^4.3.0",
    "debug": "^2.2.0",
    "domtastic": "^0.11.1",
    "draft-js": "^0.7.0",
    "events": "^1.0.2",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "globify": "^1.2.1",
    "jade": "^1.11.0",
    "moment": "^2.10.6",
    "mongodb": "^2.0.48",
    "node-neat": "^1.7.2",
    "nodemailer": "^1.11.0",
    "nodemailer-ses-transport": "^1.4.0",
    "object-assign": "^4.0.1",
    "passport": "^0.4.0",
    "passport-saml": "^0.31.0",
    "pkgify": "^1.0.3",
    "plumber": "^0.4.8",
    "postcss-calc": "^5.0.0",
    "postcss-cli": "^2.5.2",
    "postcss-color-function": "^2.0.0",
    "postcss-extend": "^1.0.1",
    "postcss-import": "^7.1.3",
    "postcss-mixins": "^3.0.2",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^1.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-color": "^2.2.0",
    "react-dom": "^16.5.2",
    "react-dropzone": "^3.5.3",
    "react-masonry-component": "^3.0.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.2.1",
    "redux-ignore": "^1.2.3",
    "redux-thunk": "^1.0.3",
    "redux-undo": "^0.6.1",
    "require-dir": "^0.3.0",
    "reselect": "^2.5.3",
    "run-sequence": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "sinon": "^1.17.3",
    "supertest": "^3.1.0",
    "uglifyify": "^3.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.11.0",
    "xr": "^0.1.13"
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery",
        "$"
      ]
    },
    "ga": "global:ga"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "chokidar": "^2.0.4",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2"
  }
}
